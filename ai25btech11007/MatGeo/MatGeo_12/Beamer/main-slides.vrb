        import numpy as np
# Inverse of A
A_inv = np.array([
    [1, -1, 0],
    [-2, 3, -4],
    [-2, 3, -3]
])

# Vector b
b = np.array([21, 20, 5])
# Solution x = A_inv * b
x = np.dot(A_inv, b)

print("Solution of the system:")
print(f"x = {x[0]}")
print(f"y = {x[1]}")
print(f"z = {x[2]}")
    \end{lstlisting}
